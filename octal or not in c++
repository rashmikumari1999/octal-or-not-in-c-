#include <iostream>
using namespace std;
main()
{
    int num,oct=0,place=1,rem=0; 
    cout<<"enter a dec number";
    cin>>num;
    cout<<"octal equivalent is"<<num;
    while(num)
    {
        rem=place%8;
    oct=rem*place+oct;
    num = num/8;
    place=place*10;
    
}
cout<<oct;
return 0;
}
